

t @TimeScale { units: second, };

C @Compartment 'C' { units: litre, boundary: true, } .= 0.95;

S1 @Species 'S1' { units: mole/litre, compartment: C, } .= 1;
S2 @Species 'S2' { units: mole/litre, compartment: C, } .= 0.5;
S3 @Species 'S3' { units: mole/litre, compartment: C, } .= 0;

reaction1 @Reaction 'reaction1' { actors: S1 = S2 + S3, };
reaction1 := C * (kf * S1 + (-1) * kr * S2 * S3);

kf @Const 'kf' { } = 2.5;
kr @Const 'kr' { } = 0.2;

