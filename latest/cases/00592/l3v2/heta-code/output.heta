

t @TimeScale { units: second, };

C @Compartment 'C' { units: metre, boundary: true, } .= 1;

S1 @Species 'S1' { units: mole/metre, compartment: C, } .= 0.15;
S2 @Species 'S2' { units: mole/metre, compartment: C, } .= 0;

reaction1 @Reaction 'reaction1' { actors: S1 = S2, reversible: false, };
reaction1 := C * k1 * S1;

k1 @Const 'k1' { } = 1;

