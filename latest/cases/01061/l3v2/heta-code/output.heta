

t @TimeScale { units: second, };

C @Compartment 'C' { units: litre, boundary: true, } .= 0.5;

S1 @Species 'S1' { units: mole/litre, compartment: C, } .= 1;
S2 @Species 'S2' { units: mole/litre, compartment: C, } .= 0;

reaction1 @Reaction 'reaction1' { actors: S2 = S1, };
reaction1 := (-(C * (kf * S1 + (-1) * kr * S2)));

kf @Const 'kf' { } = 0.9;
kr @Const 'kr' { } = 0.075;

