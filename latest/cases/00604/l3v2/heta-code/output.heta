
multiply #defineFunction { arguments: [x, y], math: x * y, };

t @TimeScale { units: second, };

C @Compartment 'C' { units: litre, boundary: true, } .= 2.3;

S1 @Species 'S1' { units: mole/litre, compartment: C, } .= 0.0015;
S2 @Species 'S2' { units: mole/litre, compartment: C, } .= 0.0015;

reaction1 @Reaction 'reaction1' { actors: S1 = S2, reversible: false, };
reaction1 := C * multiply(k1, S1);

k1 @Const 'k1' { } = 1;

