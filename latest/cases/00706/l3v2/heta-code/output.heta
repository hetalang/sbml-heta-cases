

t @TimeScale { units: second, };

C @Compartment 'C' { units: litre, boundary: true, } .= 1;

S1 @Species 'S1' { units: mole/litre, compartment: C, } .= 0.133333333333333 * k;
S2 @Species 'S2' { units: mole/litre, compartment: C, } .= 0.15 / C;

reaction1 @Reaction 'reaction1' { actors: S1 = S2, reversible: false, };
reaction1 := C * k__reaction1_local * S1;

k @Record 'k' { };
k := 0.75;

k__reaction1_local @Const { } = 0.75;

