

t @TimeScale { units: second, };

S1_proc @Process { actors:  = S1, aux: {metaid: "rule1"}, };
S1_proc := (-1) * factorial(ceil(p1 * S1)) * pow(p2, (-1));
S2_proc @Process { actors:  = S2, aux: {metaid: "rule2"}, };
S2_proc := factorial(ceil(p1 * S1)) * pow(p2, (-1));

S1 @Record 'S1' { } .= 1;
S2 @Record 'S2' { } .= 0;

p1 @Const 'p1' { } = 4;
p2 @Const 'p2' { } = 25;

