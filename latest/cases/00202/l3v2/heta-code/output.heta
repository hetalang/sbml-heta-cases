

t @TimeScale { units: second, };

compartment @Compartment 'compartment' { units: metre^2, boundary: true, } .= 0.6;

S1 @Species 'S1' { units: mole/metre^2, compartment: compartment, } .= 1.5e-6 / compartment;
S2 @Species 'S2' { units: mole/metre^2, compartment: compartment, } .= 0 / compartment;

reaction1 @Reaction 'reaction1' { actors: S1 = S2, reversible: false, };
reaction1 := compartment * k1 * S1;

k1 @Const 'k1' { } = 1.1;

