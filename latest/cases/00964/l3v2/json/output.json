[
  {
    "id": "t"
  },
  {
    "id": "S",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "ode_": "Q + R + T"
    }
  },
  {
    "id": "Q",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Qinc": "Q + 0.01"
    }
  },
  {
    "id": "R",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Rinc": "R + 0.01"
    }
  },
  {
    "id": "T",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Tinc": "T + 0.01"
    }
  },
  {
    "id": "reset",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Qinc": "t",
      "Rinc": "t",
      "Tinc": "t"
    }
  },
  {
    "id": "tolerance",
    "class": "Const",
    "num": 5
  },
  {
    "id": "error",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "error_check": "1"
    }
  },
  {
    "id": "Qinc",
    "class": "DSwitcher",
    "trigger": "(t - reset) >= 0.01"
  },
  {
    "id": "Rinc",
    "class": "DSwitcher",
    "trigger": "(t - reset) >= 0.01"
  },
  {
    "id": "Tinc",
    "class": "DSwitcher",
    "trigger": "(t - reset) >= 0.01"
  },
  {
    "id": "error_check",
    "class": "DSwitcher",
    "trigger": "abs(Q - R) >= tolerance or abs(Q - T) >= tolerance or abs(T - R) >= tolerance"
  }
]