

t @TimeScale { units: second, };

C @Compartment 'C' { units: litre, boundary: true, } .= 1;

S1 @Species 'S1' { units: mole/litre, compartment: C, } .= 0.0015 / C;
S2 @Species 'S2' { units: mole/litre, compartment: C, } .= p1 * S1 * t;

reaction1 @Reaction 'reaction1' { actors: S1 = S2, reversible: false, };
reaction1 := C * k1 * S1;

k1 @Const 'k1' { } = 1;
p1 @Const 'p1' { } = -0.5;

