[
  {
    "id": "t"
  },
  {
    "id": "S",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "ode_": "Q + R"
    }
  },
  {
    "id": "Q",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Qinc": "Q + 0.01"
    }
  },
  {
    "id": "R",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Rinc": "R + 0.01"
    }
  },
  {
    "id": "reset",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Qinc": "t",
      "Rinc": "t"
    }
  },
  {
    "id": "Qrun",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Qinc": "Qrun + 1",
      "Rinc": "0"
    }
  },
  {
    "id": "Rrun",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "Qinc": "0",
      "Rinc": "Rrun + 1"
    }
  },
  {
    "id": "maxrun",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "maxrun_check1": "Qrun",
      "maxrun_check2": "Rrun"
    }
  },
  {
    "id": "error",
    "class": "Record",
    "assignments": {
      "start_": "0",
      "error_check": "1"
    }
  },
  {
    "id": "Qinc",
    "class": "DSwitcher",
    "trigger": "(t - reset) >= 0.01"
  },
  {
    "id": "Rinc",
    "class": "DSwitcher",
    "trigger": "(t - reset) >= 0.01"
  },
  {
    "id": "maxrun_check1",
    "class": "DSwitcher",
    "trigger": "Qrun > maxrun"
  },
  {
    "id": "maxrun_check2",
    "class": "DSwitcher",
    "trigger": "Rrun > maxrun"
  },
  {
    "id": "error_check",
    "class": "DSwitcher",
    "trigger": "maxrun > 300"
  }
]